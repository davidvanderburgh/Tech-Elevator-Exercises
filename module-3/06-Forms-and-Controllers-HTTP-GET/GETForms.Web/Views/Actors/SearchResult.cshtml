@model ActorSearch

@{
    ViewData["Title"] = "SearchResult";
}

<form asp-controller="Actors" asp-action="SearchResult" method="get">
    <div>
        <label asp-for="LastNameSearchString"></label>
        <input type="text" asp-for="LastNameSearchString" />
        <input type="submit" value="Search" />
    </div>
</form>

<h2>Name</h2>
@foreach (Actor actor in Model.Actors)
{
    <p>@actor.FirstName @actor.LastName</p>
}

